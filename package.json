  "author": "Seyed Morteza Mirbostani",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4000",
    "start": "next start -p 4000",
    "build": "next build",
    "postbuild": "next-sitemap --config sitemap.config.js",
