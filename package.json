    "next-seo": "^4.26.0",
    "next-sitemap": "^1.6.168",
    "next-translate": "^1.0.7",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^7.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-headroom": "^3.1.1",
